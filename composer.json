{
  "name": "josh/laravel-doctor",
  "description": "Artisan-first Laravel codebase health scanner with diagnostics and scoring",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/console": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Josh\\LaravelDoctor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Josh\\LaravelDoctor\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Josh\\LaravelDoctor\\LaravelDoctorServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
